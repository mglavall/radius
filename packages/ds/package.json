{
  "name": "ds",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "prebuild": "yarn icons",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "storybook": "start-storybook -p 6006",
    "prebuild-storybook": "yarn icons",
    "build-storybook": "build-storybook --docs",
    "chromatic": "CHROMATIC_APP_CODE=$CHROMATIC_APP_CODE chromatic --exit-zero-on-changes",
    "icons": "svgr --template ./icon-template.js --index-template ./icon-index-template.js --ext tsx --icon --filename-case 'kebab' --title-prop --no-prettier --ref -d ./src/components/icons ./src/components/icons"
  },
  "peerDependencies": {
    "polished": ">=4.1.1",
    "react": ">=17",
    "styled-components": ">=5",
    "styled-system": ">=5",
    "@styled-system/css": ">=5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/ds.esm.js",
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@storybook/addon-a11y": "^6.1.20",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-docs": "^6.1.20",
    "@storybook/addon-google-analytics": "^6.1.20",
    "@storybook/addon-knobs": "^6.1.20",
    "@storybook/addon-links": "^6.1.20",
    "@storybook/addon-viewport": "^6.1.20",
    "@storybook/addons": "^6.1.20",
    "@storybook/react": "^6.1.20",
    "@styled-system/css": "^5.1.5",
    "@svgr/cli": "^5.5.0",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/styled-components": "^5.1.7",
    "@types/styled-system": "^5.1.10",
    "babel-loader": "8.1.0",
    "camelcase": "^6.2.0",
    "jest": "26.6.3",
    "husky": "^5.1.2",
    "polished": "^3.4.4",
    "react": "^16.12.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "storybook-chromatic": "^3.5.5",
    "styled-components": "^5.0.0",
    "styled-system": "^5.1.4",
    "ts-loader": "^8.0.17",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.2"
  }
}
